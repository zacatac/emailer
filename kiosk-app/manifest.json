{
    "manifest_version": 2,
    "name": "Browser Sample",
    "minimum_chrome_version": "24.0.1307.0",
    "version": "2.0",
    "icons": {
	"16": "icon_16.png",
	"128": "icon_128.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
    "app": {
	"background": {
	    "scripts": ["main.js"]
	}
    },
    "permissions": [
	"webview",
	"videoCapture",
    ]
}
